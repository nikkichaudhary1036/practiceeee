import java.util.*;
class car{
    void display() {
        string brand;
        int year;
        System.out.println("brand: " + brand);
        System.out.println("year: " + year);
        
    }
}
public class main{
    public static void main(string[] args){
        car c1 = new car();
        c1.brand("tata");
        c1.year(2022);
        c1.display();
    }
}
